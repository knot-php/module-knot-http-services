  "require": {
    "php": ">=7.2",
    "knot-lib/di": "~0.2",
    "knot-lib/http-services": "~0.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.15",
    "php-coveralls/php-coveralls": "^2.0",
    "knot-php/module-nyholm-psr7": "~0.5",
    "knot-php/module-knot-di": "~0.3"
  },
  "autoload": {
    "psr-4": {
      "knotphp\\module\\knothttpservice\\": "src/"
    }
