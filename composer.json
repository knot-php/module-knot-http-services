{
  "name": "knot-php/module-knot-http-services",
  "description": "kot-lib/http-services installer modules",
  "keywords": ["PHP","HTTP","Module","Installer","kNot Framework"],
  "homepage": "https://github.com/knot-php/module-knot-http-services",
  "license": "MIT",
  "authors": [
    {
      "name": "Katsuki Shuto",
      "email": "stk2k@knot-framework.com"
    }
